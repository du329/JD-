yShopList","result","errno","data","length","console","log","name","setup"],"sourceRoot":"","sources":["C:\\Users\\52919\\Desktop\\jd_lqs\\src\\views\\Home\\NearByShops.vue"],"sourcesContent":["<template>\r\n  <div class=\"nearByShops\">\r\n    <h2>附近店铺</h2>\r\n    <div v-for=\"shop in nearByShopList\" :key=\"shop._id\" class=\"shop\">\r\n      <div class=\"shop__Logo\">\r\n        <img :src=\"shop.imgUrl\" />\r\n      </div>\r\n      <div class=\"shop__content\">\r\n        <div class=\"title\">{{ shop.name }}</div>\r\n        <div class=\"tags\">\r\n          <span>月售{{ shop.sales }}+</span>\r\n          <span>起送￥{{ shop.expressLimit }}</span>\r\n          <span>基础运费￥{{ shop.expressPrice }}</span>\r\n        </div>\r\n        <div class=\"slogan\">{{ shop.slogan }}</div>\r\n        <div class=\"gapLine\"></div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive } from \"@vue/reactivity\";\r\nimport { get } from \"../../utils/request\";\r\n\r\nconst useNearByShopEffect = () => {\r\n  let nearByShopList = reactive([]);\r\n  const getNearByShopList = async () => {\r\n    const result = await get(\"/shop/hot-list\");\r\n    if (result?.errno === 0 && result?.data?.length) {\r\n      nearByShopList = result.data;\r\n    }\r\n    console.log(nearByShopList);\r\n  };\r\n  getNearByShopList();\r\n  return { nearByShopList };\r\n};\r\n\r\nexport default {\r\n  name: \"NearByShops\",\r\n  setup() {\r\n    const { nearByShopList } = useNearByShopEffect();\r\n    console.log(nearByShopList);\r\n    return { nearByShopList };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n\r\n.nearByShops {\r\n  padding: 0.18rem 0.18rem 0.49rem;\r\n  h2 {\r\n    margin-bottom: 0.14rem;\r\n    font-size: 0.18rem;\r\n  }\r\n  .shop {\r\n    display: flex;\r\n    margin-bottom: 0.12em;\r\n    height: 0.86rem;\r\n    &__Logo {\r\n      margin-right: 0.16rem;\r\n      width: 0.56rem;\r\n      height: 0.56rem;\r\n      img {\r\n        width: 100%;\r\n      }\r\n    }\r\n    &__content {\r\n      flex: 1;\r\n      font-size: 0.13rem;\r\n      .title {\r\n        margin-bottom: 0.08rem;\r\n        font-size: 0.16rem;\r\n      }\r\n      .tags {\r\n        margin-bottom: 0.08rem;\r\n        span {\r\n          margin-right: 0.16rem;\r\n        }\r\n      }\r\n      .slogan {\r\n        margin-bottom: 0.12rem;\r\n        color: #e93b3b;\r\n      }\r\n      .gapLine {\r\n        border-bottom: 0.01rem solid $content-bgColor;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"]}]}