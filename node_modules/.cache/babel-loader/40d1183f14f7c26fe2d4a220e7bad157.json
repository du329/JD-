ge = reactive({ username: \"\", password: \"\" });\r\n    const router = useRouter();\r\n    const handleLogin = () => {\r\n      axios(\r\n        \"https://www.fastmock.site/mock/e3fc6bf2e6c7a0233e485c036f7e016a/lqs_jd/user/login\",\r\n        {\r\n          method: \"post\",\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n          },\r\n          // 请求头：\r\n          params: {\r\n            username: loginMessage.username,\r\n            password: loginMessage.password,\r\n          },\r\n          // 请求体：\r\n\r\n        }\r\n      )\r\n        .then(() => {\r\n          localStorage.isLogin = true;\r\n          router.push({ name: \"Home\" });\r\n          alert(\"登录成功\");\r\n        })\r\n        .catch((err) => {\r\n          throw new Error(err);\r\n        });\r\n    };\r\n    return { loginMessage, handleLogin };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n.loginLayout {\r\n  padding: 1.2rem 0.48rem 0;\r\n  .userLogo {\r\n    margin: 0 auto 0.4rem;\r\n    width: 0.66rem;\r\n    height: 0.66rem;\r\n    img {\r\n      width: 100%;\r\n    }\r\n  }\r\n  .loginMessage {\r\n    .username,\r\n    .pwd {\r\n      margin-bottom: 0.16rem;\r\n      padding-left: 0.16rem;\r\n      width: 2.95rem;\r\n      height: 0.48rem;\r\n      background: #f9f9f9;\r\n      border: 1px solid rgba(0, 0, 0, 0.1);\r\n      outline-color: #0091ff;\r\n      border-radius: 0.06rem;\r\n      &::placeholder {\r\n        font-weight: 500;\r\n        font-size: 0.16rem;\r\n        color: $color;\r\n        letter-spacing: 0;\r\n        line-height: 24px;\r\n      }\r\n    }\r\n    .pwd {\r\n      margin-bottom: 0.32rem;\r\n    }\r\n    .submit {\r\n      margin-bottom: 0.16rem;\r\n      width: 2.95rem;\r\n      height: 0.48rem;\r\n      line-height: 24px;\r\n      text-align: center;\r\n      font-size: 0.16rem;\r\n      background: #0091ff;\r\n      color: #ffffff;\r\n      border: 0;\r\n    